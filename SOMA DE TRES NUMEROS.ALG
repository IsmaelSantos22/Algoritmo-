algoritmo "soma de tres numeros"
var
n1,n2,n3,soma,soma2: inteiro
inicio
escreval ("digite o um numero: ")
leia (n1)
escreval ("digite o segundo numero: ")
leia (n2)
escreval ("digite o ultimo numero: ")
leia (n3)
soma <- n1 + n2
soma2 <- soma + n3
escreval ("o resultado é = ",soma2,".")

fimalgoritmo